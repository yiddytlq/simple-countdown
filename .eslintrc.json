{
    "env": {
        "browser": true,
        "es6": true,
        "jest": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:@typescript-eslint/recommended",
        "plugin:react-hooks/recommended",
        "airbnb"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "react-hooks",
        "@typescript-eslint"
    ],
    "rules": {
        // resolved "no-console": "off",
        // resolved "linefeed": "off",
        // resolved "linebreak-style": "off",
        // resolved "operator-linebreak": "off",
        // resolved "arrow-parens": "off",
        // resolved "comma-dangle": "off",
        // resolved "no-use-before-define": "off",
        // resolved "import/prefer-default-export": "off",
        // resolved "prefer-destructuring": "off",
        // resolved "no-param-reassign": "off",
        // resolved "semi": "off",
        // resolved "arrow-body-style": "off",
        // resolved "react/jsx-filename-extension": "off",
        // resolved "no-unused-vars": "off",
        // resolved "prefer-const": "off",
        // resolved "quotes": "off",
        // resolved "react/prop-types": "off",
        // resolved "react/no-array-index-key": "off"
    },
    "settings": {
        "react": {
            "version": "detect"
        },
        "import/resolver": {
            "node": {
                "extensions": [".js", ".jsx", ".ts", ".tsx"]
            }
        }
    }
    
}
